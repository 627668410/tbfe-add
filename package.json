{
	"name": "tbfe-add",
	"displayName": "tbfe-add",
	"description": "code generation",
	"publisher": "lujing",
	"version": "0.0.5",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:tbfe-add.tbfe-add-base",
		"onCommand:tbfe-add.tbfe-add-list",
		"onCommand:tbfe-add.tbfe-add-modal",
		"onCommand:tbfe-add.tbfe-find-img"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tbfe-add.tbfe-find-img",
				"title": "tbfe-find-img"
			},
			{
				"command": "tbfe-add.tbfe-add-base",
				"title": "tbfe-add-base"
			},
			{
				"command": "tbfe-add.tbfe-add-list",
				"title": "list"
			},
			{
				"command": "tbfe-add.tbfe-add-modal",
				"title": "modal"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "tbfe-add.tbfe-add-base",
					"group": "navigation"
				}
			]
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.56.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "14.x",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"acorn": "^8.0.1",
		"acorn-class-fields": "^0.3.7",
		"acorn-jsx": "^5.3.1",
		"acorn-stage3": "^4.0.0",
		"camel-case": "^4.1.1",
		"chalk": "^4.1.0",
		"change-case": "^4.1.1",
		"commander": "^6.1.0",
		"download-git-repo": "^3.0.2",
		"espree": "^7.3.0",
		"espress": "0.0.0",
		"esprima": "^4.0.1",
		"figlet": "^1.5.0",
		"files": "^2.1.1",
