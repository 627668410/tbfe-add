{
	"name": "tbfe-add",
	"displayName": "tbfe-add",
	"description": "code generation",
	"publisher": "lujing",
	"version": "0.0.8",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:tbfe-add-page",
		"onCommand:tbfe-add-components",
		"onCommand:tbfe-add-reducers",
		"onCommand:tbfe-add-actions"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tbfe-add-page",
				"title": "tbfe-add-page"
			},
			{
				"command": "tbfe-add-components",
				"title": "tbfe-add-components"
			},
			{
				"command": "tbfe-add-reducers",
				"title": "tbfe-add-reducers"
			},
			{
				"command": "tbfe-add-actions",
				"title": "tbfe-add-actions"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "tbfe-add-page",
					"group": "navigation@1"
				},
				{
					"command": "tbfe-add-components",
					"group": "navigation@2"
				},
				{
					"command": "tbfe-add-reducers",
					"group": "navigation@3"
				},
				{
					"command": "tbfe-add-actions",
					"group": "navigation@3"
				}
			]
		},
		"configuration": {
			"type": "object",
			"title": "tbfe-add",
			"properties": {
				"tbfe-add-page": {
					"description": "生成的page指令的路径，文件夹名称用TbfeAddFileName代替"
				},
				"tbfe-add-components": {
					"description": "生成的components指令的路径，文件夹名称用TbfeAddFileName代替"
				},
				"tbfe-add-reducers": {
					"description": "生成的reducers指令的路径，文件夹名称用TbfeAddFileName代替"
				},
				"tbfe-add-actions": {
					"description": "生成的actions指令的路径，文件夹名称用TbfeAddFileName代替"
				}
			}
		}
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.56.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "14.x",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0",
		"acorn": "^8.0.1",
		"acorn-class-fields": "^0.3.7",
		"acorn-jsx": "^5.3.1",
		"acorn-stage3": "^4.0.0",
		"camel-case": "^4.1.1",
		"chalk": "^4.1.0",
		"change-case": "^4.1.1",
		"commander": "^6.1.0",
		"download-git-repo": "^3.0.2",
		"espree": "^7.3.0",
		"espress": "0.0.0",
		"esprima": "^4.0.1",
		"figlet": "^1.5.0",
		"files": "^2.1.1",
		"handlebars": "^4.7.6",
		"handlebars-delimiters": "^1.0.0",
		"inquirer": "^7.3.3",
		"metalsmith": "^2.3.0",
		"mkdirp": "^1.0.4",
		"open": "^7.3.1",
		"ora": "^5.1.0",
		"pad": "^3.2.0",
		"recast": "^0.20.3",
		"rimraf": "^3.0.2",
		"shelljs": "^0.8.4",
		"simple-git": "^2.20.1",
		"superagent": "^6.1.0",
		"user-home": "^2.0.0"
	},
	"dependencies": {
		"mkdirp": "^1.0.4",
		"shelljs": "^0.8.4"
	}
}