{
	"name": "tbfe-add",
	"displayName": "tbfe-add",
	"description": "code generation",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.56.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:tbfe-add.tbfe-add-base",
		"onCommand:tbfe-add.tbfe-add-list",
		"onCommand:tbfe-add.tbfe-add-modal"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "tbfe-add.tbfe-add-base",
				"title": "base"
			},
			{
				"command": "tbfe-add.tbfe-add-list",
				"title": "list"
			},
			{
				"command": "tbfe-add.tbfe-add-modal",
				"title": "modal"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"group": "navigation",
					"submenu": "tbfe-add/type"
				}
			],
			"tbfe-add/type": [
				{
					"command": "tbfe-add.tbfe-add-base",
					"group": "navigation"
				},
				{
					"command": "tbfe-add.tbfe-add-list",
					"group": "navigation"
				},
				{
					"command": "tbfe-add.tbfe-add-modal",
					"group": "navigation"
				}
			]
		},
		"submenus": [
			{
				"id": "tbfe-add/type",
				"label": "tbfe-add"
			}
		]
	},
	"scripts": {
		"lint": "eslint .",
		"pretest": "npm run lint",
		"test": "node ./test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.56.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "14.x",
		"eslint": "^7.19.0",
		"glob": "^7.1.6",
		"mocha": "^8.2.1",
		"typescript": "^4.1.3",
		"vscode-test": "^1.5.0"
	},
	"dependencies": {
		"mkdirp": "^1.0.4",
		"shelljs": "^0.8.4"
	}
}