		"rimraf": "^3.0.2",
		"shelljs": "^0.8.4",
		"simple-git": "^2.20.1",
		"superagent": "^6.1.0",
		"user-home": "^2.0.0"
	},
	"dependencies": {
		"mkdirp": "^1.0.4",
		"shelljs": "^0.8.4"
	}
